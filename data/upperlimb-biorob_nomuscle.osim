				<KshapeActive>0.5</KshapeActive>
				<!--exponential shape factor for passive force-length relationship-->
				<KshapePassive>4</KshapePassive>
				<!--force-velocity shape factor-->
				<Af>0.3</Af>
				<!--maximum normalized lengthening force-->
				<Flen>1.8</Flen>
			</Thelen2003Muscle>
